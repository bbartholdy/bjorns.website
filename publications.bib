@article{rubioDiscriminationChewing2019,
  title = {Discrimination between Chewing of Coca Leaves or Drinking of Coca Tea and Smoking of ``Paco'' (Coca Paste) by Hair Analysis. {{A}} Preliminary Study of Possibilities and Limitations},
  author = {Rubio, N. C. and Krumbiegel, F. and Pragst, F. and Thurmann, D. and Nagel, A. and Zytowski, E. and Aranguren, M. and Gorlelo, J. C. and Poliansky, N.},
  year = {2019},
  month = apr,
  journal = {Forensic Science International},
  volume = {297},
  pages = {171--176},
  issn = {0379-0738},
  doi = {10.1016/j.forsciint.2019.01.040},
  abstract = {Background Hair analysis is a suitable way to discriminate between coca chewers and consumers of manufactured cocaine using the coca alkaloids hygrine (HYG) and cuscohygrine (CUS) as markers. In the present preliminary study it was examined whether CUS and HYG can be detected in hair of occasional and moderate coca chewers or coca tea drinkers, whether CUS and HYG appear in hair of PACO consumers (smoking coca paste waste), and whether anhydroecgonine methyl ester (AEME) is a useful cocaine smoking marker in this context. Method Three groups were included: 10 volunteers from Buenos Aires with occasional or moderate chewing of coca leaves or drinking coca tea, 20 Argentinean PACO smokers and 8 German cocaine users. The hair samples (1\textendash 4 segments) were analyzed by a validated LC\textendash MS/MS method for cocaine (COC), norcocaine (NC), benzoylecgonine (BE), ecgonine methyl ester (EME), cocaethylene (CE), cinnamoylcocaine (CIN), tropacocaine (TRO), AEME, CUS and HYG. For comparison, eight samples of coca leaves or coca tea were analyzed. Results Only low concentrations of COC were found in hair of seven occasional users of coca leaves or coca tea (0.010\textendash 0.051\,ng/mg). For three moderate chewers of coca leaves all compounds were detected including AEME but except TRO. The hair samples of PACO smokers contained much higher concentrations of COC (0.027\textendash 341\,ng/mg, mean 37.4\,ng/mg) and its metabolites. CUS was not found in these samples but traces of HYG were seen in 8 of 37 hair segments. AEME as a marker for coca smoking was detected in hair of 15 smokers. In comparison to COC, the concentrations of EME and CIN were higher for PACO smokers than for German cocaine consumers. AEME (56\,{$\pm$}\,20\,{$\mu$}g/g) was detected in all coca leave and coca tea samples which explains the detection of this substance in hair of coca chewers. Therefore, its use for differentiation between coca chewers and PACO smokers is limited. Conclusion CUS remains to be the most suitable marker in hair for chewing coca leaves or drinking coca tea more frequently than two times per month since it does not appear in hair of Argentinean PACO smokers and German cocaine users. Contrary to a previous proposal, the ratios CIN/COC and EME/COC appeared not to be applicable as criteria for this purpose because of the higher concentration of these alkaloids in hair of PACO smokers. More research is needed to assess the value of AEME in hair of South American coca leave or cocaine users.},
  langid = {english},
  keywords = {Anhydroecgonine methyl ester,Chewing coca leaves,Cocaine,Cuscohygrine,Hygrine,PACO smoker}
}

@article{ruiz-mallenWhiteLab2018,
  title = {From {{White Lab Coats}} and {{Crazy Hair}} to {{Actual Scientists}}: {{Exploring}} the {{Impact}} of {{Researcher Interaction}} and {{Performing Arts}} on {{Students}}' {{Perceptions}} and {{Motivation}} for {{Science}}},
  shorttitle = {From {{White Lab Coats}} and {{Crazy Hair}} to {{Actual Scientists}}},
