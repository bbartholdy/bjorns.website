@article{jangBoneInvolvement2002,
  title = {Bone Involvement in Chronic Rhinosinusitis Assessed by {{99mTc-MDP}} Bone {{SPECT}}},
  author = {Jang, Y.j. and Koo, T.w. and Chung, S.y. and Park, S.g.},
