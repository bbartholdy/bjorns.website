@article{holcombThermalDecomposition1980,
  title = {Thermal Decomposition of Human Tooth Enamel},
  author = {Holcomb, D. W. and Young, R. A.},
  year = {1980},
  month = dec,
  journal = {Calcified Tissue International},
  volume = {31},
  number = {1},
  pages = {189},
  issn = {0171-967X, 1432-0827},
  doi = {10.1007/BF02407181},
  abstract = {Further insight into human tooth enamel, dense fraction (TE), has been obtained by following the change and loss of CO:I-'-, O H - , structurally incorporated H20, CI-, and, indirectly, HPO4"-'- after T E had been heated in N2 or vacuum in the range 25-1000\textasciitilde{} Quantitative infrared spectroscopic, lattice parameter, and thermogravimetric measures were used. Loss of the CO32- components begins at much lower temperature (e.g., 100\textasciitilde{} than previously recognized, which has implications for treatments in vitro and possibly in vivo. C O l - in B sites is lost continuously from the outset: the amount in A sites first decreases and then increases above 200\textasciitilde{} to a maximum at -800\textasciitilde{} ({$>$} 10\% of the possible A sites filled), where it is responsible for an increase in a lattice parameter. A substantial fraction of the COl- in B sites moves to A sites before being evolved, apparently via a CO2 intermediary. This implies an interconnectedness of the A and B sites which may be significant in vivo. No loss of C1- was observed at temperatures below 700800\textasciitilde{} Structural OH- content increases -70\% to a maximum near 400\textasciitilde{} Structurally incorporated water is lost continuously up to -800\textasciitilde{} with a sharp loss at 250-300\textasciitilde{} The "'sudden" a lattice parameter contraction, -0.014A, occurs at a kineticsdependent temperature in the 250-300\textasciitilde{} range and is accompanied by reordering and the \textasciitilde 'sharp" loss of -I/3 of the structurally incorporated H.\_,O. The hypothesis that structurally incorporated H20 is the principal cause of the enlargement of the a lattice parameter of TE compared to hydroxyapatite (9.44 vs 9.42A) is thus allowed by these experimental results.},
  langid = {english}
}

