@article{bergerOralBiofilms2018,
  title = {Oral {{Biofilms}}: {{Development}}, {{Control}}, and {{Analysis}}},
  shorttitle = {Oral {{Biofilms}}},
  author = {Berger, Daniela and Rakhamimova, Aviva and Pollack, Andrew and Loewy, Zvi},
  year = {2018},
