@article{klepingerCalculusCoca1977,
  title = {Prehistoric Dental Calculus Gives Evidence for Coca in Early Coastal {{Ecuador}}},
  author = {Klepinger, Linda L. and Kuhn, John K. and Thomas, Josephus},
  year = {1977},
  month = oct,
  journal = {Nature},
  volume = {269},
  number = {5628},
  pages = {506--507},
  publisher = {{Nature Publishing Group}},
  issn = {1476-4687},
  doi = {10.1038/269506a0},
  abstract = {Los CERRITOS, a late Chorrera (Engoroy) cemetery on the Santa Elena Peninsula of Ecuador yielded artefacts and radiocarbon assay which indicate that use of the site began about 840 BC and continued for several years within the Late Formative1. The human skulls from the site are of particular interest because many showed heavy accumulations of supragingival dental calculus (tartar), which contrast with the relatively slight calculus accumulations on the teeth of skulls recovered from the earlier Valdivia (3000\textendash 1500 BC) site of Real Alto2, also on the Santa Elena Peninsula. We argue here that the heavy calculus accumulations are the result of habitual, secular coca chewing in late Chorrera times. This diagnosis, in turn, supports the hypothesis of prehistoric trade in coca, probably in exchange for mollusc shells, with inhabitants of the eastern Andean slopes.},
  copyright = {1977 Nature Publishing Group},
  langid = {english},
  keywords = {calculus}
}

