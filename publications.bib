@article{bermudezUreaseActivityDental2011,
  title = {Urease Activity in Dental Plaque and Saliva of Children during a Three-Year Study Period and Its Relationship with Other Caries Risk Factors},
  author = {{Morou-Bermudez}, E and {Elias-Boneta}, A and Billings, RJ and Burne, RA and {Garcia-Rivas}, V and {Brignoni-Nazario}, V and {Suarez-Perez}, E},
