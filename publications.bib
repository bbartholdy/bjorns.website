@article{tomczykDentalCaries,
  title = {Dental Caries and Isotope Studies in the Population of {{Radom}} ({{Poland}}) between the 11th and 19th Centuries},
  author = {Tomczyk, Jacek and Szostek, Krzysztof and Lisowska-Gaczorek, Aleksandra and Mnich, Barbara and Zalewska, Marta and Trzeciecki, Maciej and Olczak-Kowalczyk, Dorota},
  journal = {International Journal of Osteoarchaeology},
  volume = {n/a},
  number = {n/a},
  issn = {1099-1212},
  doi = {10.1002/oa.2908},
  abstract = {This study aims to assess changes in the diet of individuals from Radom (Poland) by applying odontological and physicochemical methods. We evaluated the intensity of dental caries and quality of diet through isotope analyses ({$\delta$}13C and {$\delta$}15N). Three consecutive time periods were selected: the early Medieval (EMP, 11th to 12th c.), late Medieval/early Modern (LMP, 14th to 17th c.) and Modern (MP, 18th to19th c.) periods. The dental remains of 247 adult individuals comprising a total of 3,850 permanent teeth were analysed. The intensity of dental caries increased in subsequent historical periods (EMP 38\%, LMP 47\% and MP 47\%). The lowest proportion of terrestrial C3 diet was noted in the oldest EMP population. The LMP and MP were similar in terms of the type of diet. The proportion of the terrestrial C3 component of the diet was 82\% and 79\% for LMP and MP, respectively. It can be supposed that the EMP population had such a varied diet that cariogenic products (e.g., cereals) were not the basic source of food, thus slowing down the development of dental caries. Later, there was a clear increase in the cariogenic factor(s) between the EMP and LMP, and this may have been related to the intensification of agricultural production.},
  copyright = {\textcopyright{} 2020 John Wiley \& Sons, Ltd.},
  langid = {english},
  keywords = {dental caries,diet,isotope analyses,Radom},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/oa.2908}
}

