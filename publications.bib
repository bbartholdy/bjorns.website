@article{potharstTwoAlgorithms2009,
  title = {Two Algorithms for Generating Structured and Unstructured Monotone Ordinal Data Sets},
  author = {Potharst, Rob and {Ben-David}, Arie and {van Wezel}, Michiel},
  year = {2009},
  month = jun,
  journal = {Engineering Applications of Artificial Intelligence},
  volume = {22},
