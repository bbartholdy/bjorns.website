@article{brozouLeprosyMedieval2021,
  title = {Leprosy in Medieval {{Denmark}}: {{Exploring}} Life Histories through a Multi-Tissue and Multi-Isotopic Approach},
  shorttitle = {Leprosy in Medieval {{Denmark}}},
  author = {Brozou, Anastasia and Fuller, Benjamin T. and Grimes, Vaughan and Lynnerup, Niels and Boldsen, Jesper L. and J{\o}rkov, Marie Louise and Pedersen, Dorthe Dangvard and Olsen, Jesper and Mannino, Marcello A.},
  year = {2021},
  journal = {American Journal of Physical Anthropology},
  volume = {176},
  number = {1},
  pages = {36--53},
  issn = {1096-8644},
  doi = {10.1002/ajpa.24339},
  abstract = {Objectives By focusing on two Danish leprosaria (N\ae stved and Odense; 13th\textendash 16th c. CE) and using diet and origin as proxies, we follow a multi-isotopic approach to reconstruct life histories of patients and investigate how leprosy affected both institutionalized individuals and the medieval Danish community as a whole. Materials and Methods We combine archaeology, historical sources, biological anthropology, isotopic analyses ({$\delta$}13C, {$\delta$}15N, {$\delta$}34S, 87Sr/86Sr) and radiocarbon dating, and further analyze bones with different turnover rates (ribs and long bones). Results The {$\delta$}13C, {$\delta$}15N and {$\delta$}34S results indicate a C3 terrestrial diet with small contributions of marine protein for leprosy patients and individuals from other medieval Danish sites. A similar diet is seen through time, between males and females, and patients with and without changes on facial bones. The isotopic comparison between ribs and long bones reveals no significant dietary change. The {$\delta$}34S and 87Sr/86Sr results suggest that patients were local to the regions of the leprosaria. Moreover, the radiocarbon dates show a mere 50\% agreement with the arm position dating method used in Denmark. Conclusions A local origin for the leprosy patients is in line with historical evidence, unlike the small dietary contribution of marine protein. Although only 10\% of the analyzed individuals have rib/long bone offsets that undoubtedly show a dietary shift, the data appear to reveal a pattern for 25 individuals (out of 50), with elevated {$\delta$}13C and/or {$\delta$}15N values in the ribs compared to the long bones, which points toward a communal type of diet and reveals organizational aspects of the institution.},
  langid = {english},
  keywords = {bone turnover,diet,mobility,radiocarbon dating,stable isotopes},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/ajpa.24339}
}

