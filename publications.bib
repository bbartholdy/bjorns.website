  author = {K{\"o}hler, K. and P{\'a}lfi, Gy and Moln{\'a}r, E. and Zalai-Ga{\'a}l, I. and Oszt{\'a}s, A. and B{\'a}nffy, E. and Kirin{\'o}, K. and Kiss, K. K. and Mende, B. G.},
