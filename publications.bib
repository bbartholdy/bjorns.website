@article{busuiocRoleIntracellular2009,
  title = {Role of {{Intracellular Polysaccharide}} in {{Persistence}} of {{Streptococcus}} Mutans},
  author = {Busuioc, Monica and Mackiewicz, Katarzyna and Buttaro, Bettina A. and Piggot, Patrick J.},
  year = {2009},
