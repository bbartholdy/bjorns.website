@article{sorrentinoInfluenceMobility2020,
  title = {The Influence of Mobility Strategy on the Modern Human Talus},
  author = {Sorrentino, Rita and Stephens, Nicholas B. and Carlson, Kristian J. and Figus, Carla and Fiorenza, Luca and Frost, Stephen and Harcourt-Smith, William and Parr, William and Saers, Jaap and Turley, Kevin and Wroe, Stephen and Belcastro, Maria G. and Ryan, Timothy M. and Benazzi, Stefano},
