@article{denyscTaphonomyExperimentation2002,
  title = {Taphonomy and Experimentation},
  author = {Denys C},
  year = {2002},
  journal = {Archaeometry},
  volume = {44},
  number = {3},
  pages = {469--484},
  doi = {doi:10.1111/1475-4754.00079},
  abstract = {Many experiments have been conducted in the field of taphonomy in order to understand the effects of some agents and the associated pattern and process of bone modifications. A review of these experiments and the patterns of bone modifications is presented here. Numerous experiments focus on peculiar points of bone accumulation, mainly to gain knowledge about human habits and practices, but the action of some taphonomic agents has also been explored, among them, diagenetic processes. The striking feature of all these works are that they are localized, performed on non\textendash statistical samples and without repeatability. Very few experiments combine different agents to fit more closely to the complexity of most fossil bone accumulations.}
}

