@article{stoudtCollaborativeWriting2022,
  title = {Collaborative {{Writing Workflows}} in the {{Data-Driven Classroom}}: {{A Conversation Starter}}},
  shorttitle = {Collaborative {{Writing Workflows}} in the {{Data-Driven Classroom}}},
  author = {Stoudt, Sara},
  year = {2022},
